<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.nacos.demo.mapper.UserMapper">

    <select id="findById" resultType="com.nacos.demo.model.User">
        SELECT
            id,
            name
        FROM
            `user`
        WHERE
            id = #{id}
    </select>

</mapper>